{
  "family": "node-docker-app-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::941914179834:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "node-docker-app",
      "image": "941914179834.dkr.ecr.us-east-2.amazonaws.com/node-docker-app:latest",
      "portMappings": [{"containerPort":3000,"protocol":"tcp"}],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/node-docker-app",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}

